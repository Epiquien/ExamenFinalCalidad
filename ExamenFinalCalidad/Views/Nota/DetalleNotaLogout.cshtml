
<script src="~/js/DetalleCurso.js"></script>

<h1 style="color:white">Curso de @ViewBag.Nota.TituloNota</h1>


<table class="table table-borderless" style="color:white">
    <thead>
        <tr>
            <th>Nombre de nota</th>
            <th>Fecha de creacion</th>
            <th>Creador</th>
        </tr>
    </thead>
    <tbody>

        <tr>
            <td>@ViewBag.Nota.TituloNota</td>
            <td>@ViewBag.Nota.Fecha</td>
            <td>@ViewBag.Usuario.Nombres</td>
        </tr>

    </tbody>
</table>

<a href='javascript:history.go(-1)' class="btn btn-info btnRegresarDetalle"><i class="fas fa-long-arrow-alt-left"></i> Regresar</a>